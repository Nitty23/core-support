---
layout: null
---
[
  {% for post in site.posts %}
    {
      "title"      : "{{ post.title | escape }}",
      "tags"       : "{{ post.tags | join: ', ' }}",
      "url"        : "{{ site.baseurl }}{{ post.url }}",
      "topicid"    : "{{ post.topicid }}",
      "tohome"     : "{{ post.tohome }}",
      "topictitle" : "{{ post.topictitle }}",
      "summary"    : "{{ post.summary }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]