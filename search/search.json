---
layout: null
---
[
  {% for post in site.posts %}
    {
      "title"      : "{{ post.title | escape }}",
      "url"        : "{{ site.baseurl }}{{ post.url }}",
      "excerpt"    : "{{ post.content | strip | strip_newlines | strip_html | truncatewords: 50 }}",
      "tags"       : "{{ post.tags }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]
